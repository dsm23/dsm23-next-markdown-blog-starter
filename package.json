{
  "name": "dsm23-next-markdown-blog-starter",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "node .husky/install.mjs",
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint:js": "eslint \"**/*.{js,mjs,ts,tsx}\" --report-unused-disable-directives --max-warnings 0",
    "lint:md": "eslint \"**/*.md?(x)\" --report-unused-disable-directives --max-warnings 0 --cache --cache-strategy=content --cache-location=.eslintmdcache",
    "lint:css": "stylelint \"**/*.css\" --allow-empty-input --cache --cache-strategy=content --cache-location=.stylelintcache",
    "lint": "pnpm run lint:md && pnpm run lint:js && pnpm run lint:css"
  },
  "dependencies": {
    "@next/bundle-analyzer": "15.0.3",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@tailwindcss/postcss": "4.0.0-alpha.24",
    "@types/node": "22.9.0",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "@vercel/analytics": "^1.3.2",
    "@vercel/speed-insights": "^1.1.0",
    "geist": "1.3.1",
    "next": "15.0.3",
    "next-mdx-remote": "^5.0.0",
    "postcss": "^8.4.47",
    "react": "19.0.0-rc-65a56d0e-20241020",
    "react-dom": "19.0.0-rc-65a56d0e-20241020",
    "sugar-high": "^0.7.5",
    "tailwindcss": "4.0.0-alpha.24",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@eslint/compat": "^1.2.2",
    "@eslint/js": "^9.14.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@playwright/test": "^1.48.2",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "9.14.0",
    "eslint-config-next": "15.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-storybook": "^0.11.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "globals": "^15.12.0",
    "gzip-size": "^7.0.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "mkdirp": "^3.0.1",
    "prettier": "^3.3.3",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "stylelint-selector-bem-pattern": "^4.0.1",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.13.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.1",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
    }
  }
}
